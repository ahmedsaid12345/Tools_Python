<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>Runnable_lAB_2</SHORT-NAME>
			<!-- Learn_In_Depth Runnable_Lab_1 Session -->
			<ELEMENTS>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRIF_OUTPUT</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>SUM_Result</SHORT-NAME>
							<TYPE-TREF DEST="ABSTRACT-IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>

				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRIF_Inputs</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Var_1</SHORT-NAME>
							<TYPE-TREF DEST="ABSTRACT-IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>

						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Var_2</SHORT-NAME>
							<TYPE-TREF DEST="ABSTRACT-IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>

					</DATA-ELEMENTS>

				</SENDER-RECEIVER-INTERFACE>

				<!-- SWC -->

				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SWC_1</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_SUM</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SRIF_OUTPUT</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>

						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_Sum</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SRIF_Inputs</REQUIRED-INTERFACE-TREF>

						</R-PORT-PROTOTYPE>
					</PORTS>
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>SWC1_Behaviour</SHORT-NAME>
						<EXCLUSIVE-AREAS>
						<EXCLUSIVE-AREA>
						<SHORT-NAME>EXCLUSIVE_AREA</SHORT-NAME>
						
						</EXCLUSIVE-AREA>
						
						</EXCLUSIVE-AREAS>
						<EVENTS>
						<TIMING-EVENT>
						<SHORT-NAME>timing_event_10_ms</SHORT-NAME>
						<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWC1/SWC1_Behaviour/SUM_Runnable</START-ON-EVENT-REF>
						<PERIOD>.01</PERIOD>
						</TIMING-EVENT>
						
						<INIT-EVENT>
						<SHORT-NAME>Init_Event</SHORT-NAME>
						<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWC1/SWC1_Behaviour/SUM_Runnable</START-ON-EVENT-REF>
						</INIT-EVENT>
						<DATA-RECEIVED-EVENT>
						<SHORT-NAME>Data_Received_COMPLETE_Event</SHORT-NAME>
						<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWC1/SWC1_Behaviour/SUM_Runnable</START-ON-EVENT-REF>
						
						
						<DATA-IREF>
						<TARGET-DATA-ELEMENT-REF DEST="VARIABLE-DATA-PROTOTYPE">/SRIF_Inputs/Var_1</TARGET-DATA-ELEMENT-REF>
						</DATA-IREF>
						
						</DATA-RECEIVED-EVENT>
						<DATA-WRITE-COMPLETED-EVENT>
						<SHORT-NAME>Data_Write_COMPLETE_Event</SHORT-NAME>
						<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/SWC1/SWC1_Behaviour/SUM_Runnable</START-ON-EVENT-REF>
						<EVENT-SOURCE-REF DEST="VARIABLE-ACCESS">/SWC1/SWC1_Behaviour/SUM_ResultData</EVENT-SOURCE-REF>
						</DATA-WRITE-COMPLETED-EVENT>
						</EVENTS>
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>SUM_Runnable</SHORT-NAME>

									<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
									<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>

									<DATA-READ-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>VAR_1</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="R-PORT-PROTOTYPE">/SWC_1/RP_Sum</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/SRIF_Inputs/Var_1</TARGET-DATA-PROTOTYPE-REF>

												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
										<VARIABLE-ACCESS>
										<SHORT-NAME>VAR_2</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="R-PORT-PROTOTYPE">/SWC_1/RP_Sum</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/SRIF_Inputs/Var_2</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>

											</ACCESSED-VARIABLE>


										</VARIABLE-ACCESS>
									</DATA-READ-ACCESSS>





									<DATA-WRITE-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>SUM_ResultData</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="P-PORT-PROTOTYPE">/SWC_1/PP_SUM</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/SRIF_OUTPUT/SUM_Result</TARGET-DATA-PROTOTYPE-REF>

												</AUTOSAR-VARIABLE-IREF>

											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-WRITE-ACCESSS>


									<SYMBOL>SUM</SYMBOL>

								</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>

				</APPLICATION-SW-COMPONENT-TYPE>

			</ELEMENTS>
		</AR-PACKAGE>


	</AR-PACKAGES>


</AUTOSAR>