<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>SWC_Composition_lAB</SHORT-NAME>
			<!-- Learn_In_Depth Composition_Lab Session -->
			<ELEMENTS>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_Light_Control</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Light_Control</SHORT-NAME>
							<TYPE-TREF DEST="ABSTRACT-IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_Light_Actuator</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Light_Actuator</SHORT-NAME>
							<TYPE-TREF DEST="ABSTRACT-IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>Light_control_Status_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Pp_SR_light_Control</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SWC_Composition_lAB/SRI_Light_Control</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Algo_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>Rp_SR_Light_Control</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SWC_Composition_lAB/SRI_Light_Control</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>

						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Pp_SR_Light_Actuator</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SWC_Composition_lAB/SRI_Light_Actuator</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</APPLICATION-SW-COMPONENT-TYPE>

				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>Light_Acuator_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_SR_LightActuator</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SWC_Composition_lAB/SRI_Light_Actuator</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<!-- Composite SWC -->
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SW_Composite_1</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Pp_SR_Light_Actuator_Composite</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SWC_Composition_lAB/SRI_Light_Actuator</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LC_SA_SWC1</SHORT-NAME>
							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/Light_control_Status_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>Light_Algo_SWC_2</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/Algo_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>AS_Light_Control</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/SW_Composite_1/LC_SA_SWC1</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/Light_control_Status_SWC/Pp_SR_light_Control</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/SW_Composite_1/Light_Algo_SWC_2</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/Algo_SWC/Rp_SR_Light_Control</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
						<DELEGATION-SW-CONNECTOR>
							<SHORT-NAME>De_LightActuator</SHORT-NAME>
							<INNER-PORT-IREF>
								<P-PORT-IN-COMPOSITION-INSTANCE-REF>
									<CONTEXT-COMPONENT-REF
										DEST="SW-COMPONENT-PROTOTYPE">/SW_Composite_1/Light_Algo_SWC_2</CONTEXT-COMPONENT-REF>
									<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/Algo_SWC/Pp_SR_Light_Actuator</TARGET-P-PORT-REF>
								</P-PORT-IN-COMPOSITION-INSTANCE-REF>
							</INNER-PORT-IREF>
							<OUTER-PORT-REF DEST="P-PORT-PROTOTYPE">/SW_Composite_1/Pp_SR_Light_Actuator_Composite</OUTER-PORT-REF>
						</DELEGATION-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Top_Level_Composite_SWC</SHORT-NAME>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>SW_Composite_Instant</SHORT-NAME>
							<TYPE-TREF DEST="COMPOSITION-SW-COMPONENT-TYPE">/SW_Composite_1</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LightActuate_SWC3</SHORT-NAME>
							<TYPE-TREF DEST="SW-COMPONENT-TYPE">/Light_Acuator_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>Assembly_Light_Actuator</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/Top_Level_Composite_SWC/SW_Composite_Instant</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/SW_Composite_1/Pp_SR_Light_Actuator_Composite</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/Top_Level_Composite_SWC/LightActuate_SWC3</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/Light_Acuator_SWC/RP_SR_LightActuator</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>